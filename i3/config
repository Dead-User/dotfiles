mode "layout" {
bindsym t focus left
bindsym n focus right
bindsym r focus up
bindsym n focus down
bindsym p focus parent
bindsym c focus child
bindsym Shift+t move left
bindsym Shift+n move right
bindsym Shift+r move up
bindsym Shift+n move down
bindsym minus resize shrink width 10 px or 10 ppt
bindsym equal resize grow width 10 px or 10 ppt
bindsym Shift+minus resize shrink height 10 px or 10 ppt
bindsym Shift+equal resize grow height 10 px or 10 ppt
bindsym Control+s layout stacking
bindsym Control+t layout tabbed
bindsym Control+h layout splith
bindsym Control+v layout splitv
bindsym f floating toggle
bindsym a move_to_workspace focus prev
bindsym o move_to_workspace focus next
bindsym 6 workspace 6:net
bindsym Shift+6 move container to workspace 6:net
bindsym 5 workspace 5:sys
bindsym Shift+5 move container to workspace 5:sys
bindsym 3 workspace 3:emu
bindsym Shift+3 move container to workspace 3:emu
bindsym 2 workspace 2:edit
bindsym Shift+2 move container to workspace 2:edit
bindsym 1 workspace 1:web
bindsym Shift+1 move container to workspace 1:web
bindsym 4 workspace 4:media
bindsym Shift+4 move container to workspace 4:media
bindsym Shift+a move container to workspace prev
bindsym Shift+o move container to workspace next
bindsym Control+1 move workspace to output eDP1
bindsym Control+2 move workspcae to output DP1
bindsym k move workspace to output left
bindsym j move workspace to output right
bindsym Escape mode "default"
bindsym Return mode "default"
bindsym Mod4+l mode "default"
}
bindsym Mod4+l mode "layout"
bindsym Mod4+Control+r reload
bindsym Mod4+Control+x restart
bindsym Mod4+Control+q exec new-terminal --exec exit-i3
mode "floating" {
bindsym t resize shrink width 10 px or 10 ppt
bindsym n resize grow width 10 px or 10 ppt
bindsym r resize shrink height 10 px or 10 ppt
bindsym h resize grow height 10 px or 10 ppt
bindsym Return mode "default"
bindsym Escape mode "default"
bindsym Mod4+c mode "default"
}
mode "menu" {
bindsym r exec amixer set Master 5%+
bindsym h exec amixer set Master 5%-
bindsym t exec mpc prev
bindsym n exec mpc next
bindsym space exec mpc toggle
bindsym q exec xbacklight -inc 10
bindsym a exec xbacklight -dec 10
bindsym Return mode "default"
bindsym Escape mode "default"
bindsym Mod4+c mode "default"
}
bindsym Mod4+m mode "menu"
